/*
Given a string, return true if it ends in "ly".

endsLy("oddly") → true
endsLy("y") → false
endsLy("oddy") → false
*/

public class Main
{
  public static void main(String[] args) {
    System.out.println(endsly("Helly"));
  }

  public static boolean endsLy(String str) {
    return str.endsWith("ly");
  }
}
